{
  "name": "mytips",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "update:generate": "node scripts/generate-update-manifest.js",
    "update:build": "yarn build && TAURI_SIGNING_PRIVATE_KEY=$HOME/.tauri/myapp.key yarn tauri build && yarn update:generate",
    "update:build:windows": "yarn build && TAURI_SIGNING_PRIVATE_KEY=$HOME/.tauri/myapp.key bun tauri build --runner cargo-xwin --target x86_64-pc-windows-msvc && yarn update:generate",
    "update:build:all": "yarn update:build && yarn update:build:windows",
    "update:deploy": "bash dist/update-manifests/deploy.sh",
    "release": "yarn update:build && yarn update:deploy",
    "release:all": "yarn update:build:all && yarn update:deploy"
  },
  "dependencies": {
    "@tanstack/vue-virtual": "^3.13.12",
    "@tauri-apps/api": "^2.7.0",
    "@tauri-apps/plugin-autostart": "~2",
    "@tauri-apps/plugin-clipboard-manager": "^2.3.0",
    "@tauri-apps/plugin-dialog": "~2",
    "@tauri-apps/plugin-fs": "~2",
    "@tauri-apps/plugin-global-shortcut": "~2",
    "@tauri-apps/plugin-process": "^2.2.2",
    "@tauri-apps/plugin-shell": "^2.3.0",
    "@tauri-apps/plugin-updater": "^2.8.1",
    "@tauri-apps/plugin-window-state": "~2",
    "@types/diff-match-patch": "^1.0.36",
    "@types/prismjs": "^1.26.5",
    "@vueuse/core": "^10.7.0",
    "date-fns": "^2.30.0",
    "diff-match-patch": "^1.0.5",
    "dompurify": "^3.2.6",
    "highlight.js": "^11.9.0",
    "lru-cache": "^11.1.0",
    "marked": "^15.0.12",
    "marked-highlight": "^2.2.1",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^4.4.1",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.30.0",
    "v-calendar": "^3.1.2",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.10",
    "vue-router": "^4.2.5",
    "vue-virtual-scroller": "^2.0.0-beta.8",
    "vue3-toastify": "^0.2.8"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@tauri-apps/cli": "^2.5.0",
    "@types/dompurify": "^3.0.5",
    "@types/markdown-it": "^14.1.2",
    "@types/marked": "^5.0.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "autoprefixer": "^10.4.16",
    "daisyui": "^4.4.19",
    "install": "^0.13.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "~5.6.2",
    "vite": "^6.0.3",
    "vue-tsc": "^2.1.10"
  }
}
