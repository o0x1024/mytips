{
  "name": "mytips",
  "private": true,
  "version": "1.1.6",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "update:generate": "node scripts/generate-update-manifest.js",
    "update:build": "yarn build && TAURI_SIGNING_PRIVATE_KEY=$HOME/.tauri/myapp.key yarn tauri build && yarn update:generate",
    "update:build:windows": "yarn build && TAURI_SIGNING_PRIVATE_KEY=$HOME/.tauri/myapp.key bun tauri build --runner cargo-xwin --target x86_64-pc-windows-msvc && yarn update:generate",
    "update:build:all": "yarn update:build && yarn update:build:windows",
    "update:deploy": "bash dist/update-manifests/deploy.sh",
    "release": "yarn update:build && yarn update:deploy",
    "release:all": "yarn update:build:all && yarn update:deploy"
  },
